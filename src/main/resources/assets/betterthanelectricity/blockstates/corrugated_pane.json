{
    "multipart": [
        {   "apply": { "model": "betterthanelectricity:corrugated_pane_post_ends" }},
        {   "apply": { "model": "betterthanelectricity:corrugated_pane_post" }},
        {   "when": { "north": true },
            "apply": { "model": "betterthanelectricity:corrugated_pane_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "betterthanelectricity:corrugated_pane_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "betterthanelectricity:corrugated_pane_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "betterthanelectricity:corrugated_pane_side_alt", "y": 90 }
        }
    ]
}
